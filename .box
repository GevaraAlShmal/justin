general: 
    writable_directories: [ "/wp-content/cache/", "/wp-content/uploads" ]
    
php: 
    version: 5.3.5
    error_reporting: E_ALL
    date_timezone: US/central
    max_execution_time: 30
    max_input_time: 60
    post_max_size: 8M
    file_uploads: On
    upload_max_filesize: 20M
    max_file_uploads: 200